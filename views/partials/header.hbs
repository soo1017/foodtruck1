{{# if adminHeader}}
    {{# if logoOut}}
    {{else}}
    <nav class="navbar nav-header admin-nav-header">
<!--    <nav class="navbar navbar-expand-lg navbar-light bg-light nav-header">-->
        <div class="admin-mainlogo" id="navbarSupportedContent">  
            <a href="/" class="navbar-brand"><img src="/images/logo.png" alt="uptaste logo" class=""></a>
        </div>
    </nav>
    {{/if}}
{{else}}

<nav class="navbar nav-header">
