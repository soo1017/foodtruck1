                    <li><a class="nav-link" href="/#id-menus">MENUS</a></li>
                    <li><a class="nav-link" href="/shop">ORDER</a></li>
                    {{# if login }}
                    <li><a class="nav-link" href="/user/profile">USER</a></li>
                    <li><a class="nav-link" href="/user/logout">SIGNOUT</a></li>
                    {{ else }}
                    <li><a class="nav-link" href="/user/signup">SIGNUP</a></li>
                    <li><a class="nav-link" href="/user/signin">SIGNIN</a></li>
                    {{/if}}
                </ul>
            </div>
        </div>
        <div class="navbar-menu">
            <ul class="navbar-menu-ul non-mobile">
                <li class="nav-item active">
                    <a class="nav-link" href="/#id-foodtruck"><img src="/images/food-truck.png" alt="ftruck logo" class=""></a>
                </li>
                <li class="nav-item active">
                    <a class="nav-link" href="/#id-menus">MENUS</a>
                </li>
                <li class="nav-item active">
                    <a class="nav-link" href="/shop">ORDER</a>
                </li>
            </ul>
        </div>
