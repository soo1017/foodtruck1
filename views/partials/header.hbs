            <a href="/" class="navbar-brand"><img src="/images/logo.png" alt="uptaste logo" class=""></a>
        </div>
        <div class="navbar-conn">
            <ul class="navbar-conn-ul">
<!--            <ul class="navbar-nav my-2 my-lg-0">-->
                <li class="nav-item dropdown active cls-cart">
                    <div class="navcart" id="id-navcart">
                        <img src="/images/cart.png" alt="Smiley face" class="cart-image">
                        <span class='badge badge-warning cartcount'>{{session.cart.totalQty}}</span>
                    </div>
    <!--                -->
                    
    <!--                -->
                </li>

                <li class="nav-item dropdown active cls-user non-mobile">
    <!--                <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fas fa-user"></i> User</a>-->
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><img src="/images/user.png" alt="Smiley face" height="44" width="44"></a>
                    <div class="dropdown-menu user" aria-labelledby="navbarDropdown">
                        {{# if login }}
                            <a class="dropdown-item" href="/user/profile">User Account</a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" href="/user/logout">SignOut</a>
                        {{ else }}
                            <a class="dropdown-item" href="/user/signup">SignUp</a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" href="/user/signin">SignIn</a>
                        {{/if}}
                    </div>
                </li>
            </ul>
        </div>
<!--                -->
                    {{#if cartOpen}}
                    <div id="id-shoppingcart" class="shoppingcart shows">
                    {{else}}
                    <div id="id-shoppingcart" class="shoppingcart hide">
                    {{/if}}
                        <div class="yes-items">
                            <div class="shoppingcart-head">
                                <p>
                                    <span id="count-cart">{{totalQty}}</span>
                                    {{# if Qtys}}
                                    <span id="item-cart">items</span>
                                    {{else}}
                                    <span id="item-cart">item</span>
                                    {{/if}}
                                </p>
                                <span id="id-cartclose">&times;</span>
                            </div>
                            <div class="shoppingcart-ready">
                                <p class="ready-cart">Ready within 20-25 min</p>
                            </div>

                            <div class="shoppingcart-content">
                                <ul class="content-ul" id="content-ul">
                                    <!---->
                                    {{#if currentUrl}}
                                        {{#each cartItems}}
                                        <li>
                                            <p>{{this.item.name}}</p>
                                            <a href="/shop/remove-one-fromcart/{{this.item._id}}"><ion-icon name="close-circle" id="id-item-delete"></ion-icon></a>
                                            <p class="content-price">{{this.item.price}}</p>
                                        </li>
                                        {{/each}}
                                    {{else}}
                                        {{#each cartItems}}
                                        <li>
                                            <p>{{this.item.name}}</p>
                                            <a href="/remove-one-fromcart/{{this.item._id}}"><ion-icon name="close-circle" id="id-item-delete"></ion-icon></a>
                                            <p class="content-price">{{this.item.price}}</p>
                                        </li>
                                        {{/each}}
                                    {{/if}}
                                </ul>
                            </div>
                            <hr style="width:90%">
                            <div class="shoppingcart-pay">
                                <p>You Pay<span id="id-youpay">${{totalPrice}}</span></p>
                            </div>
                            {{# if cartItems}}
                            <div class="shoppingcart-button">
                            {{else}}
                            <div class="shoppingcart-button hide">
                            {{/if}}
                                {{# if login }}
                                    <div>
                                        <a href="/shop/checkout" class="btn btn-success" id="id-checkout-btn">Proceed to Checkout</a>
                                    </div>
                                {{else}}
                                    <div>
                                        <a href="/shop/checkout" class="btn btn-success" id="id-checkout-btn">Proceed with SignIn</a>
                                    </div>
                                    <div>
                                        <a href="/shop/checkout-guest" class="btn btn-outline-success" id="id-checkout-btn">Proceed with a Guest</a>
                                    </div>
                                {{/if}}
                                <br>
                                <div>
                                    {{#if currentUrl}}
                                    <a href="/shop/clear-cart" class = "clearcart">Clear Cart</a>
                                    {{else}}
                                    <a href="/clear-cart" class = "clearcart">Clear Cart</a>
                                    {{/if}}
                                </div>
                            </div>
                        </div>
                    </div>
        
    </div>
    
</nav>
{{/if}}
    
